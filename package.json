{
  "name": "craft-and-local-api",
  "version": "1.0.0",
  "description": "API for Craft & Local",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "7.7.4"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production",
    "prod:start": "cross-env NODE_ENV=production heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow",
    "heroku-postbuild": "yarn prod:build",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-flow-react-proptypes": "^3.4.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.2.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.50.0",
    "husky": "^0.14.3",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-polyfill": "^6.23.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.2",
    "mongoose": "^4.11.3",
    "request": "^2.81.0",
    "rimraf": "^2.6.1"
  }
}
